<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_hasil_vote"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="id.sch.smktelkom_mlg.project.xiirpl109192939.voteapp.HasilActivity">

    <TextView
        android:text="Vote :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:layout_gravity="center_horizontal"
        android:textSize="20sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="JUDUL VOTING "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewJudul"
        android:layout_gravity="center_horizontal"
        android:textSize="20sp"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView11"
        android:layout_toEndOf="@+id/textView11" />

    <TextView
        android:text="DAFTAR CALON "
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:layout_marginTop="38dp"
        android:layout_below="@+id/textView11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Nama Calon "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewNamaCalon"
        android:layout_marginTop="18dp"
        android:layout_below="@+id/textView6"
        android:layout_alignLeft="@+id/textViewJumSuara"
        android:layout_alignStart="@+id/textViewJumSuara" />

    <TextView
        android:text="Jumlah Suara :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:layout_alignBaseline="@+id/textViewJumSuara"
        android:layout_alignBottom="@+id/textViewJumSuara"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Nama Calon   :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        android:layout_alignBaseline="@+id/textViewNamaCalon"
        android:layout_alignBottom="@+id/textViewNamaCalon"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Jumlah Suara "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewJumSuara"
        android:layout_marginTop="13dp"
        android:layout_below="@+id/textView15"
        android:layout_alignRight="@+id/textViewJudul"
        android:layout_alignEnd="@+id/textViewJudul" />

</RelativeLayout>
